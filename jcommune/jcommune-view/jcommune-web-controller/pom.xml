<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>jcommune-view</artifactId>
    <groupId>org.jtalks.jcommune</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>  
  <artifactId>jcommune-web-controller</artifactId>  
  <name>${artifactId}r</name>
  <url>wwww.jtalks.org</url>
  <description>
        This module is responsible for event handling of the web-view module. 
  </description>

  <build>
  	<plugins>
  		<plugin>
  			<groupId>org.maven.ide.eclipse</groupId>
  			<artifactId>lifecycle-mapping</artifactId>
  			<version>0.9.9-SNAPSHOT</version>
  			<configuration><mappingId>generic</mappingId><configurators></configurators><mojoExecutions></mojoExecutions></configuration>
  		</plugin>
  	</plugins>
  </build>
  <dependencies>

  	<dependency>
  		<groupId>org.springframework</groupId>
  		<artifactId>spring-webmvc</artifactId>
  		<version>3.0.5.RELEASE</version>
  		<type>jar</type>
  		<scope>compile</scope>
  	</dependency>
  </dependencies>
  
  
</project>


  
  


